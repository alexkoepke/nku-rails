<div class="container" id="content">
  
  <div class="row">
    
    <div class="col-sm-6">
      <h1>Edit Profile</h1>
    </div>
  </div>
  <div class="row">
    <div>
      <%= form_for :student, url: student_path(@student), method: :patch do |f| %>
        <% if @student.errors.any? %>
          <div id="error_explanation">
            <h2>
              <%= pluralize(@student.errors.count, "error") %> prohibited this post from being saved:
            </h2>
            <ul>
              <% @student.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
              <% end %>
            </ul>
          </div>
        <% end %>
      
        <div class="row">
          <div class="form-group col-sm-4">
            <%= f.label :name, 'Name' %><br>
            <%= f.text_field :name, class: 'form-control' %>
          </div>
        </div>
        <div class="row">
          <div class="form-group col-sm-4">
            <%= f.label :nickname, 'Nickname' %><br>
            <%= f.text_field :nickname, class: 'form-control' %>
          </div>
        </div>
        <div class="row">
          <div class="form-group col-sm-4">
            <%= f.label :email, 'Email' %><br>
            <%= f.text_field :email, class: 'form-control' %>
          </div>
        </div>
        <div class="row">
          <div class="form-group col-sm-4">
            <%= f.label :image_url, 'Image URL' %><br>
            <%= f.text_field :image_url, class: 'form-control' %>
          </div>
        </div>
        <div class="row">
          <div class="form-actions col-sm-4">
            <%= f.submit %>
          </div>
        </div>
        </div>
      <% end %>
      
      <%= link_to 'Back', students_path %>
    </div>
  </div>

</div>